{
  "name": "sort-algs-node",
  "version": "1.0.0",
  "main": "index.js",
  "module": "main.js",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "e2e": "./node_modules/.bin/cypress open",
    "test": "jest",
    "rebuild": "npm clean-install && ng build",
    "ci": "npm run test --code-coverage --no-watch --no-progress --browsers=ChromeHeadless --reporters=spec,kjhtml,coverage-istanbul,junit,htmlDetailed",
    "test-ci": "ng test --code-coverage --watch=false --browsers=ChromeHeadless --reporters=spec,kjhtml,coverage-istanbul,junit,htmlDetailed",
    "coverage": "ng test --code-coverage --progress --watch=false --reporters=spec,kjhtml,coverage-istanbul,junit,htmlDetailed",
    "test-dev": "ng test --code-coverage --progress --watch=true --browsers=Chrome",
    "test-sr": "npm run test --singleRun=true",
    "lint": "eslint --ext ts src/app",
    "lint-fix": "eslint --ext ts --fix src/app",
    "lint-dry": "eslint --ext ts --fix-dry-run src/app",
    "lint-test": "eslint --ext ts src/test/**/*.ts",
    "lint-old": "ng lint",
    "e2e-report": "node e2e/cucumber-html-reporter.js",
    "e2e-prep": "node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager clean && node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager update",
    "e2e-v": "npm ls webdriver-manager",
    "e2e-versions": "npm ls webdriver-manager",
    "e2e-drivers": "node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager status"
  },
  "author": "",
  "keywords": [],
  "description": "",
  "dependencies": {
    "@types/node": "^14.0.27",
    "chai-arrays": "^2.2.0",
    "restify": "^8.5.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^26.0.9",
    "chai": "^4.2.0",
    "chai-datetime": "^1.7.0",
    "chai-http": "^4.3.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.2.0",
    "cypress": "^4.12.1",
    "cypress-cucumber-preprocessor": "^2.5.4",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "jest-html-reporter": "^3.1.3",
    "karma": "^5.1.1",
    "mocha": "^8.1.1",
    "ts-jest": "^26.1.4"
  }
}
